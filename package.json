    "react-dom": "^18.2.0",
    "react-i18next": "^15.6.1",
    "react-icons": "^5.5.0",
    "react-quill": "^2.0.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^7.6.3",
    "react-toastify": "^11.0.5",
    "recharts": "^3.1.0",
    "socket.io-client": "^4.8.1",
    "tesseract.js": "^6.0.1",
    "workbox-webpack-plugin": "^7.3.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
