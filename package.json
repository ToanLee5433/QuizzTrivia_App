    "@types/dompurify": "^3.0.5",
    "canvas-confetti": "^1.9.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dexie": "^4.0.11",
    "dompurify": "^3.3.0",
    "express": "^5.1.0",
    "firebase": "^10.14.1",
    "framer-motion": "^12.23.24",
    "genkit": "^1.22.0",
    "glob": "^11.0.3",
    "howler": "^2.2.4",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.536.0",
    "mammoth": "^1.9.1",
    "nodemailer": "^7.0.5",
    "papaparse": "^5.5.3",
    "pdfjs-dist": "^5.4.54",
    "qrcode": "^1.5.4",
    "quill": "^2.0.3",
