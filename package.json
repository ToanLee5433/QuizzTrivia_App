{
  "name": "quiz-trivia-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node ./node_modules/vite/bin/vite.js",
    "build": "tsc && node ./node_modules/vite/bin/vite.js build",
    "preview": "node ./node_modules/vite/bin/vite.js preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:warn": "eslint . --ext ts,tsx --max-warnings 999999",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "jest --config=config/jest.config.cjs",
    "test:watch": "jest --config=config/jest.config.cjs --watch",
    "test:coverage": "jest --config=config/jest.config.cjs --coverage",
    "test:ci": "jest --config=config/jest.config.cjs --ci --coverage --watchAll=false",
    "build:index": "npx tsx scripts/buildVectorIndex.ts",
    "test:rag": "npx tsx scripts/testRAG.ts",
    "i18n:validate": "node scripts/validate-i18n.mjs",
    "i18n:fix": "node scripts/fix-i18n.mjs",
    "i18n:check": "npm run i18n:validate",
    "i18n:extract": "node scripts/i18n/i18n-scanner.mjs extract",
    "i18n:sync": "node scripts/i18n/i18n-scanner.mjs sync",
    "i18n:report": "node scripts/i18n/i18n-report.mjs",
    "i18n:audit": "npm run i18n:extract && npm run i18n:validate && npm run i18n:report",
    "i18n:setup": "cd scripts/i18n && npm install",
    "i18n:rebuild": "node scripts/rebuild-vi-translations.mjs",
    "i18n:complete": "node scripts/complete-i18n-auto.mjs",
    "i18n:replace-dry": "node scripts/auto-replace-hardcoded-strings.mjs --dry-run",
    "i18n:replace": "node scripts/auto-replace-hardcoded-strings.mjs",
    "i18n:auto": "node scripts/setup-i18n-complete.mjs",
    "i18n:aggressive-dry": "node scripts/aggressive-i18n-replacer.mjs --dry-run",
    "i18n:aggressive": "node scripts/safe-i18n-replace.mjs",
    "i18n:full": "npm run i18n:complete && npm run i18n:aggressive",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
